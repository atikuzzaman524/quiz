<?php
/**
 * @file
 * Renders xAPI statements into human readable reports.
 */

/**
 * Generate report from xAPI data.
 *
 * @param $xapiData
 *
 * @return string Readable report
 */
function h5preport_generateReport($xapiData) {
  $H5PReport = H5PReport::getInstance();
  return $H5PReport->generateReport($xapiData);
}
